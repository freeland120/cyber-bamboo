<div class="board-container centered">
  <table class="board-table" border="1">
    <thead>
      <tr>
        <td id="board-id"><%#= ID %></td id="id">
        <td id="board-title">제목</td>
        <td id="board-author">작성자</td>
        <td id="board-createdAt">작성일</td>
        <td id="board-view">조회</td>
        <td id="board-like">좋아요</td>
      </tr>
    </thead>
    <tbody>
      <% if(result) { for(let i=0; i < result.length; i++){ %>

      <tr>
        <td><%= result[i].id %></td>
        <td><%= result[i].title %></td>
        <td><%= result[i].author %></td>
        <td><%= result[i].createdAt %></td>
        <td><%= result[i].views %></td>
        <td><%= result[i].like %></td>
      </tr>
      <% } } %>
    </tbody>
  </table>
</div>
